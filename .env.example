# Supabase Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# Required: Your Supabase project URL
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Required: Your Supabase anon/public key
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Production: Enable Edge Functions for admin operations (recommended)
# Set to 'true' in production, 'false' or omit in development
EXPO_PUBLIC_USE_EDGE_FUNCTIONS=false

# Development Only: Service role key for local admin operations
# WARNING: Never set this in production builds!
# Only use locally for development/testing
# SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
